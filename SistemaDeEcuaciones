// SistemaDeEcuaciones.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
 
void main()
{ float A, B, C, D, E, F, x, y;
  cout<<" Ingrese los valores de A,B,C,D,E y F: "<<endl;
   cin>>A;
   cin>>B;
   cin>>C,
   cin>>D;
   cin>>E;
   cin>>F;
   y=((A*F)-(D*E))/((A*E)- (D*B)) ;
   x=(F -(E*y))/ D ;
   cout<<" el valor de x es: "<<x;
   cout<<" el valor de y es: "<<y;
   getch();
}
