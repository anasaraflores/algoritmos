// InteresSimple.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main()
{float C, P, T, I;
 cout<< "Ingrese el monto del capital: "<<endl;
 cin>>C;
 cout<< "Ingrese el monto del periodo: "<<endl;
 cin>>P;
 cout<< "Ingrese la tasa de interes: "<<endl;
 cin>>T;
 I= (C+P+T)/360;
 cout<< "El interes es: "<<I<<endl;
 getch();
}

